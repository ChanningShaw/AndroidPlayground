<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:orientation="vertical">

  <Button
    android:id="@+id/flow_builder"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="flow_builder" />
  <Button
    android:id="@+id/flow"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="flow" />
  <Button
    android:id="@+id/channel_flow"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="channel_flow" />
  <Button
    android:id="@+id/channel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="channel" />
    <Button
      android:id="@+id/channel2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="channel2" />
  <Button
    android:id="@+id/channel_conflated"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="channel_conflated" />

    <Button
  android:id="@+id/broadcast_channel"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="broadcast_channel" />

  <Button
    android:id="@+id/channel_filter"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="channel_filter" />

  <Button
    android:id="@+id/ReceiveChannel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="ReceiveChannel" />

  <Button
    android:id="@+id/MultiReceiver"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="MultiReceiver" />

  <Button
    android:id="@+id/MultiSender"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="MultiSender" />
</LinearLayout>